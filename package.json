{
    "name": "simulationcraft",
    "displayName": "SimulationCraft",
    "description": "Language Support for SimC TCI",
    "version": "0.0.3",
    "publisher": "Mystler",
    "icon": "images/logo.png",
    "repository": {
		"type": "git",
		"url": "https://github.com/Mystler/simc-vs-code"
	},
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "simc",
            "aliases": ["SimC", "simc"],
            "extensions": [".simc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "simc",
            "scopeName": "source.simc",
            "path": "./syntaxes/simc.tmLanguage.json"
        }]
    }
}
